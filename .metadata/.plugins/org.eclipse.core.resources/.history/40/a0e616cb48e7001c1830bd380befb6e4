package com.java.virtusa;

public class NCharDecrypt {

	public static void main(String[] args) {
		String s="a1b2c3";
		String str="";
		for (int i=0; i<s.length();i++) {
			if(i%2==0) {
				for(int j=0; j<scharAt(i);j++) {
					str+=s.charAt(i-1);
				}
			}
			System.out.println(s.charAt(i));
		}
	}

}
